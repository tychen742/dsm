          "metadata": {},
          "outputs": [
            {
              "data": {
                "text/plain": [
                  "numpy.ndarray"
                ]
              },
              "execution_count": 100,
              "metadata": {},
              "output_type": "execute_result"
            }
          ],
          "source": [
            "##### guess the data type\n",
